{
  "name": "@yarond/erela.js-spotify",
  "version": "1.2.2",
  "description": "Spotify plugin for Erela.JS",
  "main": "dist/index.js",
  "types": "typings/index.d.ts",
  "files": [
    "dist",
    "typings"
  ],
  "scripts": {
    "build": "tsc",
    "types": "dts-bundle --removeSource --name @yarond/erela.js-spotify --main ./dist --out ../typings/index.d.ts --baseDir ./dist",
    "publish:stable": "yarn build && yarn publish --access=public"
  },
  "keywords": [
    "erela.js",
    "spotify",
    "plugin"
  ],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/node": "^14.6.4",
    "typescript": "^4.0.2"
  },
  "dependencies": {
    "erela.js": "https://github.com/Yarondr/erela.js",
    "petitio": "^1.3.2"
  },
  "engines": {
    "node": ">=14.x.x"
  }
}
